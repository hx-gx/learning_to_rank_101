{
  "featureset": {
    "name": "docs_features",
    "features": [
      {
        "name": "1",
        "params": [
          "keywords"
        ],
        "template": {
          "multi_match": {
            "query": "{{keywords}}",
            "fields": ["artist", "title"]
          }
        }
      },
      {
        "name": "2",
        "params": [
          "keywords"
        ],
        "template": {
          "match": {
            "artist": "{{keywords}}"
          }
        }
      },
      {
        "name": "3",
        "params": [
          "keywords"
        ],
        "template": {
          "match" : {
            "title": "{{keywords}}"
          }
        }
      },
      {
        "name": "4",
        "params": [
          "keywords"
        ],
        "template": {
          "match" : {
            "artist": "Michael Jackson"
          }
        }
      },
      {
        "name": "5",
        "params": [
          "keywords"
        ],
        "template": {
          "match" : {
            "artist": "The Police"
          }
        }
      }
    ]
  }
}



